        :generalAlerts => generalAlerts,
        :woodShopAlerts => woodShopAlerts,
        :metalShopAlerts => metalShopAlerts,
        :digitalFabricationAlerts => digitalFabricationAlerts,
        :artAlerts => artAlerts,
        :alert_ids => alert_ids
    } %>
<% elsif SS_ID == 8 %>
    <%= erb :'engineering_design_hub/alert_center', :locals => {
        :generalAlerts => generalAlerts,
        :woodShopAlerts => woodShopAlerts,
        :metalShopAlerts => metalShopAlerts,
        :digitalFabricationAlerts => digitalFabricationAlerts,
        :artAlerts => artAlerts,
        :alert_ids => alert_ids
    } %>
<% end %>
